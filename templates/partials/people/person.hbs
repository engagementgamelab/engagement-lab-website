<div class="col-sm-6 col-md-4 team-member">
	<div class="image-box style-2 mb-20 text-center">
			
		<a href="/people/{{person.key}}">
			<img id="{{cleanString person.name.first}}-{{cleanString person.name.last}}" src="{{{cloudinaryUrl person.image}}}" alt="{{person.name.first}} {{person.name.last}}" class="photo">
			
			<div class="body">
				<h3 class="theme-primary">{{person.name.first}} {{person.name.last}}</h3>
				
				{{#if person.title}}
					<p>{{person.title}}</p>
				{{/if}}
				{{#if person.cohort}}
					<p>{{person.cohort}}</p>
				{{/if}}
			</div>
		</a>

	</div>
</div>